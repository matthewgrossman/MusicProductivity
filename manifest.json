{
  "manifest_version": 2,

  "name": "Productivity",
  "description": "This extension will make you more productive",
  "version": "1.0",

  "permissions": [
    "tabs",
    "storage",
    "https://play.spotify.com/*"
  ],

  "options_page": "options.html",

  "browser_action": {
  },

  "background": {
    "scripts": ["js/jquery.min.js", "eventpage.js", "bg.js", "popup.js"],
    "persistent": false
  }
}
